    "@edsilv/utils": "0.2.6",
    "@iiif/base-component": "1.1.4",
    "@iiif/iiif-av-component": "0.0.87",
    "@iiif/iiif-gallery-component": "1.1.13",
    "@iiif/iiif-metadata-component": "1.1.13",
    "@iiif/iiif-tree-component": "1.1.12",
    "@iiif/manifold": "1.2.36",
    "@types/modernizr": "3.2.29",
    "@types/requirejs": "2.1.28",
    "@types/three": "0.84.20",
    "@universalviewer/uv-cy-gb-theme": "1.1.2",
    "@universalviewer/uv-en-gb-theme": "1.1.2",
    "core-js": "2.4.1",
    "dashjs": "2.6.7",
    "hls.js": "0.9.1",
