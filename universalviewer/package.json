    "hooker": "0.2.3",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "jest-puppeteer": "^3.5.0",
    "less": "2.4.0",
    "lodash": "4.17.21",
    "ncp": "2.0.0",
    "puppeteer": "^1.9.0",
    "serve": "^10.0.2",
    "ts-jest": "^22.4.2",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@edsilv/exjs": "0.5.1",
    "@edsilv/http-status-codes": "0.0.12",
